{
  "manifest_version": 2,
  "name": "Silver-Helper",
  "description": "This extension is adding some features to SW.",
  "version": "0.1",
  "browser_action": {
    "default_icon": "sw-icon.png",
    "default_popup": "options.html"
  },
  "icons": {
    "16": "sw-icon.png",
    "48": "sw-icon.png",
    "128": "sw-icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://www.silver-world.net/*",
        "https://www.silver-world.net/*"
      ],
      "js": [
        "jquery-3.1.1.min.js",
        "silver-helper.js"
      ]
    }
  ],
  "permissions": [
    "activeTab"
  ]
}